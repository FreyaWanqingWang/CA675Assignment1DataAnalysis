select * from posts where posts.ViewCount > 130000 ORDER BY posts.ViewCount 
select * from posts where posts.ViewCount>80000 and posts.ViewCount <= 130000 ORDER BY posts.ViewCount 
select * from posts where posts.ViewCount>58000 and posts.ViewCount <= 80000 ORDER BY posts.ViewCount 
select * from posts where posts.ViewCount>45000 and posts.ViewCount <= 58000 ORDER BY posts.ViewCount
select top 18724 * from posts where posts.ViewCount <= 45000 ORDER BY posts.ViewCount desc
